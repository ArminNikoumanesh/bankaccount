<?xml version="1.0"?>
<page title="نمایش شخص حقوقی">
        <search label="نام شرکت:"
                singleResultSearch="ws/findLegal"
                singleResultSearchInputkey="code"
                value="obj">
        </search>
    <section title="مشخصات مشتری">
        <input label="نام شرکت" type="text" value="obj.name" disable="true"/>
        <input label="نام ثبتی" type="text" value="obj.maneger" disable="true"/>
        <input label="آدرس" type="text" value="obj.address" disable="true"/>
        <input label="کدثبتی" type="text" value="obj.postalCode" disable="true"/>
        <input label="تاریخ ثبتی" type="date" value="obj.date" disable="true"/>
        <input label=" کدثبتی" type="text" value="obj.companyCode" disable="true"/>
        <table label="شماره تلفن" value="obj.numbers" type="editable">
            <input label="تلفن" type="text" value="number" disable="true"/>
            <comboBox label="نوع" value="numberTypes" disable="true">
                <comboItem itemLabel="همراه" itemValue="MOBILE"/>
                <comboItem itemLabel="ثابت" itemValue="HOME"/>
                <comboItem itemLabel="محل کار" itemValue="WORK"/>
            </comboBox>
        </table>
    </section>
</page>
